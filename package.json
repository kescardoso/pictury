{
  "name": "pictury",
  "displayName": "Pictury",
  "description": "A VS-Code extension to search and download stock-free images into a workspace",
  "version": "0.0.1",
  "publisher": "Pictury",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pictury.start",
        "title": "Open Webview",
        "category": "pictury"
      },
      {
        "command": "pictury.resize",
        "title": "Resize"
      },
      {
        "command": "pictury.toJPG",
        "title": "Convert to JPG"
      },
      {
        "command": "pictury.toPNG",
        "title": "Convert to PNG"
      },
      {
        "command": "pictury.rotateRight90",
        "title": "Rotate Right 90째"
      },
      {
        "command": "pictury.rotateRight180",
        "title": "Rotate Right 180째"
      },
      {
        "command": "pictury.rotateLeft90",
        "title": "Rotate Left 90째"
      },
      {
        "command": "pictury.rotateLeft180",
        "title": "Rotate Left 180째"
      },
      {
        "command": "pictury.downloadPathModifier",
        "title": "Set To Download Folder"
      }
    ],
    "keybindings": [
      {
        "command": "pictury.start",
        "key": "Ctrl+Alt+P",
        "mac": "Ctrl+Alt+P"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "pictury",
          "group": "navigation",
          "when": "resourceExtname == .jpg || resourceExtname == .png || resourceExtname == jpeg"
        },
        {
          "submenu": "Pictury",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        },
        {
          "submenu": "WebView",
          "group": "navigation",
          "when": "!explorerResourceIsFolder && resourceExtname != .jpg && resourceExtname != .png && resourceExtname != jpeg"
        }
      ],
      "WebView": [
        {
          "command": "pictury.start",
          "group": "navigation"
        }
      ],
      "Pictury": [
        {
          "command": "pictury.downloadPathModifier",
          "group": "navigation"
        },
        {
          "command": "pictury.start",
          "group": "navigation"
        }
      ],
      "pictury": [
        {
          "command": "pictury.resize",
          "group": "navigation"
        },
        {
          "command": "pictury.toPNG",
          "group": "navigation",
          "when": "resourceExtname == .jpeg || resourceExtname == .jpg"
        },
        {
          "command": "pictury.toJPG",
          "group": "navigation",
          "when": "resourceExtname == .png"
        },
        {
          "command": "pictury.rotateRight90",
          "group": "navigation"
        },
        {
          "command": "pictury.rotateRight180",
          "group": "navigation"
        },
        {
          "command": "pictury.rotateLeft90",
          "group": "navigation"
        },
        {
          "command": "pictury.rotateLeft180",
          "group": "navigation"
        }
      ]
    },
    "submenus": [
      {
        "id": "pictury",
        "label": "pictury"
      },
      {
        "id": "Pictury",
        "label": "pictury"
      },
      {
        "id": "WebView",
        "label": "pictury"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@types/download": "^6.2.4",
    "cross-fetch": "^3.0.6",
    "download": "^8.0.0",
    "express": "^4.17.1",
    "jimp": "^0.16.1",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1"
  }
}
